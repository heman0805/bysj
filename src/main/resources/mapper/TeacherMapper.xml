<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.heman.bysj.mapper.TeacherMapper">

   <select id="listTest" resultType="com.heman.bysj.model.entity.Teacher">
        SELECT * FROM teacher
   </select>

    <select id="selectById" parameterType="Integer" resultType="com.heman.bysj.model.entity.Teacher">
        select * from teacher where tid = #{id}
    </select>

    <select id="selectByProfessionAndPosition" resultType="com.heman.bysj.model.entity.Teacher">
        select * from teacher where profession = #{profession} AND position = #{position}
    </select>
</mapper>